{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "node",
		"outDir": "dist",
		"rootDir": ".",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["**/*.ts"],
	"exclude": ["node_modules", "dist"]
}
